@model IEnumerable<Ribbons>
<div>
    <table class="styled-table">
        <thead>
            <tr>
                <th width="130px">Tip</th>
                <th width="180px">Naziv</th>
                <th width="330px">Opis</th>
                <th width="110px">Trenutno na stanju</th>
                <th width="120px">Cena</th>
                <th width="60px"></th>
                <th width="150px"></th>
            </tr>
        </thead>
    </table>
    @foreach (var Ribbons in Model)
    {
    <div>
        <table class="styled-table">
            <tbody>
                <tr>
                    <td width="130px" align="center">@Ribbons.CbRibbons.Name</td>
                    <td width="180px" align="center">@Ribbons.Name</td>
                    <td width="330px" align="center">@Ribbons.Description</td>
                    <td width="110px" align="center">@Ribbons.CurrentNumber</td>
                    <td width="120px" align="center">@Ribbons.Price.ToString("c")</td>
                    <td width="60px" align="center"><img src="@Ribbons.Image" width="60" /></td>
                    <td width="120px" align="center"><button class="btn btn-primary">Dodaj u korpu</button>
                </tr>
            </tbody>
        </table>
    </div>
    }
</div>