@model ThermalPrinters


<div>
    <div>       
        <h2>@Model.CbBrand.Name</h2>
        <h3>@Model.Description</h3>
        <h4>@Model.Dpi</h4>
        <h5>@Model.Price.ToString("c")</h5>
        <p class="button">
            <a class="btn btn-primary"
               asp-controller="ShoppingCart"
               asp-action="AddToShoppingCart"
               asp-route-thermalPrintersIdId="@Model.Id">Dodaj u korpu</a>
        </p>
    </div>
</div>
